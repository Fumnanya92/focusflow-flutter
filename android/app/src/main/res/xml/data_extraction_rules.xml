<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- Disable cloud backup for sensitive productivity data -->
        <exclude domain="sharedpref" />
        <exclude domain="database" />
        <exclude domain="file" />
    </cloud-backup>
    <device-transfer>
        <!-- Allow device transfer for user experience -->
        <include domain="sharedpref" />
    </device-transfer>
</data-extraction-rules>